<div class="row">
    <div class="alert alert-info">
        <i class="icon-info-2"></i>
        This is an information notice for the template.
    </div>
    <div class="eightcol">
         <div class="page-header">
            <h2 class="widget widget-header">Settings</h2>
        </div>
        <form method="post" role="form" action="#">
            <fieldset class="widget widget-content widget-last">
                <div class="form-group">
                    <label for="setting_name">Store name</label>
                    <input class="form-control" id="setting_name" type="text">
                </div>
                <div class="form-group">
                    <label for="setting_email">Store email address</label>
                    <input class="form-control" id="setting_email" type="text"></input>
                </div>
                <div class="form-group">
                    <label for="setting_tax_name">Tax name</label>
                    <input class="form-control" id="setting_tax_name" type="text"></input>
                </div>
                <div class="form-group form-last">
                    <label for="setting_currency">Currency unit</label>
                    <input class="form-control" id="setting_currency" type="text"></input>
                </div>
            </fieldset>
            <div class="form-action">
                <input class="btn btn-blue btn-large" type="submit" value="Submit">
            </div>
        </form>
    </div>
    <div class="fourcol last"></div>
</div>